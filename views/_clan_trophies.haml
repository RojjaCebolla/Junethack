.scorelisting
  %table.greytable.misc
    %tr
      %th 
        Clan
      %th 
        Score
      %th 
        Rank
    - ClanScoreEntry.all(:order => [:rank.asc], :trophy => 'clan_winner').each do |entry|
      %tr
        %td
          %a{:href => "/clan/#{CGI::escape entry.clan.name}"}
            #{h entry.clan.name}
        %td.table_number
          #{h "%.2f" % entry.points}
        %td.table_number
          #{h entry.rank}
